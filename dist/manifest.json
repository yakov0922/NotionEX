{
  "manifest_version": 3,
  "name": "NotionEX",
  "version": "1.0.0",
  "default_locale": "zh_CN",
  "description": "__MSG_appDesc__",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "src/icons/16.png",
      "48": "src/icons/48.png",
      "128": "src/icons/128.png"
    }
  },
  "icons": {
    "16": "src/icons/16.png",
    "48": "src/icons/48.png",
    "128": "src/icons/128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.ts-DTJ8j7nD.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "options_ui": {
    "page": "src/options/index.html",
    "open_in_tab": true
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/index.ts-DTJ8j7nD.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
